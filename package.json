{
  "name": "apatejs-retro",
  "version": "1.0.0",
  "description": "A simple 2D retro game engine which runs directly in your browser",
  "main": "dist/apate.js",
  "scripts": {
    "build": "tsc && rollup dist/apate.js --file dist/mini.js",
    "dev": "nodemon --config nodemon.json",
    "test": "eslint src/**/*.ts && prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juiian7/ApateJS-Retro.git"
  },
  "author": "juiian7",
  "license": "GPL-3.0",
  "dependencies": {
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0"
  },
  "prettier": {
    "singleQuote": false,
    "tabWidth": 4,
    "printWidth": 200,
    "trailingComma": "es5",
    "endOfLine": "auto"
  }
}
